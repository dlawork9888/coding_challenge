def solution(n, lost, reserve):
    # 중복 처리
    lost = set(lost)
    reserve = set(reserve)
    intersection = set.intersection(lost, reserve)
    lost = set(lost) - intersection
    reserve = set(reserve) - intersection
    
    students = [1 for x in range(n + 2)]
    students[0] = -1 # 더미
    students[-1] = -1 # 더미
    for i in lost:
        students[i] = 0
    for i in reserve:
        students[i] = 2
        
    # [-1,1,2,0,1,2,1,2,0,-1]
    for i in range(1,n+1):
        if students[i] == 2:
            if students[i - 1] == 0:
                students[i] -= 1
                students[i - 1] += 1
            elif students[i + 1] == 0:
                students[i] -= 1
                students[i + 1] += 1
    count = 0
    for i in students:
        if i >= 1:
            count += 1

    return count

