def solution(number, k):
    stack = []
    for index, element in enumerate(number):
        while stack and k and stack[-1] < int(element):
            stack.pop()
            k -= 1
        stack.append(int(element))
    stack = [str(x) for x in stack]
    answer = ''.join(stack[:len(number) - k])
    return answer
    
        
        
