import copy
def return_after(n, input_adj_mat):
    adj_mat = copy.deepcopy(input_adj_mat)
    queue = deque()
    
    #시작 노드
    start_node = -1
    for i in range(n):
        for j in range(n):
            if adj_mat[i][j] == True:
                start_node = i
    if start_node == -1:
        return False
    
    queue.append(start_node)
    count = 1
    while queue:
        now_node = queue.pop()
        for i in range(n):
            if adj_mat[now_node][i] == True:
                queue.append(i)
                count += 1
                adj_mat[now_node][i] = False
                adj_mat[i][now_node] = False
    
    return count, adj_mat
        
    
