    "def process(X):\n",
    "    if X % 3 == 0:\n",
    "        X = X//3\n",
    "    elif X % 2 == 0:\n",
    "        X = X//2\n",
    "    else:\n",
    "        X -= 1\n",
    "    #print(X)\n",
    "    return X\n",
    "\n",
    "count = 0\n",
    "\n",
    "N = int(input())\n",
    "while N != 1:\n",
    "    N = process(N)\n",
    "    count += 1\n",
    "    \n",
    "print(count)"
   ]
