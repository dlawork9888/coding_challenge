    "    if dp_list[0][0] == 1:\n",
    "        print(0)\n",
    "        break\n",
    "        \n",
    "    dp_list.append([])\n",
    "    for x in dp_list[now_index]:\n",
    "        if x%3 == 0:\n",
    "            dp_list[now_index + 1].append(x//3)\n",
    "            if x//3 == 1:\n",
    "                print(now_index + 1)\n",
    "                Flag = True\n",
    "                break\n",
    "        if x%2 == 0:\n",
