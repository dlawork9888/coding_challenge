    "# 테스트 완료\n",
    "# 통합 풀이\n",
    "def d(n):\n",
    "    # 각 자리 수 산출\n",
    "    str_n = str(n)\n",
    "    for x in str_n:\n",
    "        n += int(x)\n",
    "    return n\n",
    "\n",
    "arr = [True] * 10001\n",
    "\n",
    "# 각 숫자들로 만든 수를 False 처리\n",
    "for i, bool in enumerate(arr):\n",
    "    if d(i) <= 10000:\n",
    "        arr[d(i)] = False\n",
    "\n",
    "for idx, x in enumerate(arr):\n",
    "    if x == True:\n",
    "        print(idx)"
